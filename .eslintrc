{
  "extends": "rallycoding",
  "parser": "babel-eslint",
  "rules": {
    "object-curly-spacing": 0,
    "arrow-body-style": 0,
    "camelcase":0,
    "max-len":0,
    "react/jsx-closing-bracket-location":0,
    "eol-last":0,
    "no-underscore-dangle":0,
    "no-param-reassign":0,
    "object-property-newline" :0,
    "react/sort-comp" : 0
  }
}
